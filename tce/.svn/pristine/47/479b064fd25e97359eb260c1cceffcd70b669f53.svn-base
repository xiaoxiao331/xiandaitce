@charset "utf-8";
/* CSS Document */
body,th,td,textarea,input,select,option,h1,h2,h3,h4,h5,h6 {font-size:16px; color:#666; letter-spacing:-0.5px;  font-family:Arial, Helvetica, sans-serif, "宋体", "돋움", "굴림",}
body {background-color: #fff; margin:0;}

.wrap { width:100%;}
.body { margin:auto 0; background:url(images/top_bg.png) repeat-x; height:340px; }
.content {width:1000px;  margin:auto;}
.cont  {background:url(images/bg.png) no-repeat; height:332px; }
#login_wrap {position:relative; margin:auto 0;}
.login_bg { width:292px; height:349px; background:url(images/login_bg.png) no-repeat; position:absolute; top:150px; left:580px;}
.login_cont { padding:20px;}
.login_txt { width:80%; padding-left:20px;}
.txt { padding-top:5px; padding-bottom:3px;}
.login_btn { text-align:center; padding-top:25px;}
.login_btn img { border:0;}
.input {}
.input_box {border: 0px currentColor; width: 100%; height: 25px; color: #666666; border:#c1c1c1 1px solid;}
.radio_bx { text-align:center; padding:10px;}
.radio { float:left; padding-left:10px; padding-right:10px;}
.footer { padding-top:20px; overflow:hidden; text-align:center; padding-top:300px;}