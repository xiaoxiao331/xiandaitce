/* 모달레이어팝업 */
.layerpopupView {position:fixed;top:0;left:0;width:100%;height:100%;z-index:999;}
.layerpopup .mask       {display:none; position:absolute; left:0; top:0; bottom:0; z-index:9000; width:100%; height:100%; background-color:#000;}
.layerpopup .popupbox   {display:none; position:absolute; left:50%; top:50%; z-index:10000; background-color: #fff;
                        -webkit-box-shadow: 3px 3px 3px 0px rgba(0,0,0,0.15); -moz-box-shadow: 3px 3px 3px 0px rgba(0,0,0,0.15); box-shadow: 3px 3px 3px 0px rgba(0,0,0,0.15);}
.layerpopup .popupbox .title            {position:relative; height: 70px; background-color: #004998;}
.layerpopup .popupbox .title strong     {display: inline-block; min-width:250px; height: 70px; padding-left:20px; background:url(../img/bg_poptitle.gif) no-repeat; color: #fff; font-size: 26px; line-height:70px;} 
.layerpopup .popupbox .title .btn_colse {display:block; position:absolute; top:0; right:0; width:70px; height: 70px; background: url(../img/ico_close.png) no-repeat; background-position: center; text-indent: -9999em;}
.layerpopup .popupbox .con              {padding: 30px 20px;}
                       
.layerpopup .type01                     {overflow:auto; width:940px; margin-left:-470px;}
.layerpopup .popupbox .btns .btn {width:160px;}
.layerpopup .popupbox.wid_l {width:1000px; margin-left:-500px;}
.layerpopup .popupbox.wid_m {width:700px; margin-left:-350px;}
.layerpopup .popupbox.wid_s {width:500px; margin-left:-250px;}
.layerpopup .popupbox .work_order p                 {font-size: 16px; color: #444; margin-bottom: 20px;}
.layerpopup .popupbox .work_order fieldset span     {display: block; margin-bottom:5px; color:#999; font-size: 12px;}
.layerpopup .popupbox textarea {width:100%; height: 130px; border: 1px solid #aaa; }
.layerpopup .popupbox .txt1 {color:#000; font-size: 18px; text-align: center;}
.layerpopup .popupbox .check_list {text-align: right;}
.layerpopup .popupbox .policy select {width:100%; margin-bottom: 10px; border: 1px solid #c2c2c2;}
.layerpopup .popupbox .policy > div  {overflow:auto; overflow-x:hidden; width:100%; height: 365px; padding:10px; margin-bottom:10px; border: 1px solid #c2c2c2;}
.layerpopup .popupbox .cfnum input[type=text] {background-color: #eeeeee;}
.layerpopup .popupbox .cfnum p {margin-bottom:10px; color:#000; font-size:18px; line-height:30px; font-weight:500; text-align: center;}

.layerpopup .popupbox.error {width:440px; height:270px; margin-left:-220px; border: 5px solid #004998;}
.layerpopup .popupbox.error .con .btn_close {display: block; position: absolute; top:10px; right:10px;}
.layerpopup .popupbox.error .con        {position:relative; height:100%; text-align: center;}
.layerpopup .popupbox.error .con:before {display: inline-block; width:5px; height: 100%; vertical-align: middle; content: '';  }
.layerpopup .popupbox.error .con p     {display:inline-block; color: #000; font-size: 18px; vertical-align: middle;}
.layerpopup .popupbox.error .con p img {display: block; margin:0 auto 20px auto;}

.layer2popup{position:fixed;top:0;left:0;width:100%;height:100%;}
.layer2popup .mask{display:block;opacity:0.6;}
.layer2popup .popupbox{display:block;margin-top:-135px;}

