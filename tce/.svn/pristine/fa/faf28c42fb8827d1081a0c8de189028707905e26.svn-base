@charset "utf-8";

@font-face {
	font-family: 'Noto Sans KR';
	font-weight: 100;
	src: url('../font/NotoSansCJK/NotoSansCJKkr-Thin.eot');
	src: url('../font/NotoSansCJK/NotoSansCJKkr-Thin.eot') format('embedded-opentype'),
		 url('../font/NotoSansCJK/NotoSansCJKkr-Thin.woff') format('woff'),
		 url('../font/NotoSansCJK/NotoSansCJKkr-Thin.otf') format('truetype');
}

