public class TCEProperties {
    /** 초기패스워드 */
    public static String defaultPw;
    
    /** Aria Key */
    public static String ariaKey;
    
    /** 비밀번호 실패 Count*/
    public static int pwFailCnt;
    
    /** 미접속 기간 */
    public static int loginNotDay;
    
    /** ISS URL */
    public static String issUrl;
    
    /** 테이블 생성 파일 */
    public static String queryPath;
    
    /**
     * @return the queryPath
     */
    public static String getQueryPath() {
        return queryPath;
    }

    /**
     * @param queryPath the queryPath to set
     */
    public static void setQueryPath(String queryPath) {
        TCEProperties.queryPath = queryPath;
    }
    
    /**
     * @return the defaultPw
     */
    public static String getDefaultPw() {
        return defaultPw;
    }

    /**
     * @param defaultPw the defaultPw to set
     */
    public static void setDefaultPw(String defaultPw) {
        TCEProperties.defaultPw = defaultPw;
    }

    /**
     * @return the ariaKey
     */
    public static String getAriaKey() {
        return ariaKey;
    }

    /**
     * @param ariaKey the ariaKey to set
     */
    public static void setAriaKey(String ariaKey) {
        TCEProperties.ariaKey = ariaKey;
    }

    /**
     * @return the pwFailCnt
     */
    public static int getPwFailCnt() {
        return pwFailCnt;
    }

    /**
     * @param pwFailCnt the pwFailCnt to set
     */
    public static void setPwFailCnt(int pwFailCnt) {
        TCEProperties.pwFailCnt = pwFailCnt;
    }

    /**
     * @return the loginNotDay
     */
    public static int getLoginNotDay() {
        return loginNotDay;
    }

    /**
     * @param loginNotDay the loginNotDay to set
     */
    public static void setLoginNotDay(int loginNotDay) {
        TCEProperties.loginNotDay = loginNotDay;
    }
    
    /**
     * @return the issUrl
     */
    public static String getIssUrl() {
        return issUrl;
    }

    /**
     * @param issUrl the issUrl to set
     */
    public static void setIssUrl(String issUrl) {
        TCEProperties.issUrl = issUrl;
    }
}
